generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  account   String   @id
  twitter   String?  @unique
  discord   String?
  userToken String?  @unique
  active    Boolean  @default(true)
  createdAt DateTime @default(now())
}
