{
  "name": "fweb3-faucet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "localdev": "NEXT_APP_LOCALNET=true next dev",
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_OPTIONS='--http-server-default-timeout=0' next start",
    "lint": "next lint",
    "db": "docker run --rm --name fweb3-dev-db -p 5432:5432 -e POSTGRES_PASSWORD=fweb3 -e POSTGRES_DB=fweb3_dev postgres:14-alpine",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx ts-node -O '{\"module\":\"commonjs\"}' scripts/seed.ts",
    "sync:local": "npx ts-node -O '{\"module\":\"commonjs\"}' scripts/sync.ts local",
    "sync:mumbai": "npx ts-node -O '{\"module\":\"commonjs\"}' scripts/sync.ts mumbai",
    "sync:polygon": "npx ts-node -O '{\"module\":\"commonjs\"}' scripts/sync.ts polygon"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/source-code-pro": "^4.5.6",
    "@mui/icons-material": "^5.6.1",
    "@mui/lab": "^5.0.0-alpha.77",
    "@mui/material": "^5.6.1",
    "@mui/styled-engine-sc": "^5.6.1",
    "@walletconnect/web3-provider": "^1.7.7",
    "@web3auth/web3auth": "^0.9.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "ethers": "^5.6.4",
    "magic-sdk": "^8.1.0",
    "next": "^12.2.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-google-recaptcha": "^2.1.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@types/react-google-recaptcha": "^2.1.5",
    "dotenv": "^16.0.0",
    "eslint": "^8.21.0",
    "eslint-config-next": "12.1.5",
    "fs-extra": "^10.1.0",
    "typescript": "4.6.3"
  }
}
